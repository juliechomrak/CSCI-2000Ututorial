    "import numpy as np\n",
    "\n",
    "def WhiteWash(matrix):\n",
    "    x = 0\n",
    "    for element in matrix:\n",
    "        matrix[x] = (element + 1) / 2\n",
    "        x = x+1\n",
    "    print(matrix)\n",
